
*
{
    margin: 0;
    padding: 0;
    font-family: Calibri;
}

a
{
    color: #507030;
    text-decoration: none;
}

body
{
    background: #FFFFFF;
    color: #507030;
}

h1
{
    font-size: 20px;
    font-weight: bold;
    font-family: Times New Roman;
    padding-left: 5px;
}

h2
{
    font-size: 16px;
    font-weight: bold;
    font-family: Times New Roman;
    padding: 10px 0 5px 5px;
}

hr
{
    background-color: #507030;
    height: 1px;
    border: 0px;
    margin-left: 15px;
    margin-right: 15px;
}

fieldset
{
    border: 1px solid #507030;
    padding: 15px 15px 15px 15px;
}

form
{
}

form label
{
    display: block;
    width: 165px;
    text-align: right;
    float: left;
    margin-right: 5px;
}

form ul
{
    list-style: none;
}

form ul li
{
    display: inline-block;
    width: 339px;
    padding-bottom: 5px;
}

form ul li.long
{
    width: 678px;
}

input[type="button"], input[type="submit"]
{
    border: 0px;
    background: #608840;
    color: #FFFFFF;
    padding-bottom: 2px;
    padding-top: 2px;
    width: 130px;
    height: 25px;
    font-size: 16px;
    margin-top: 5px;
    margin-bottom: 5px;
}

input[type="button"]:hover, input[type="submit"]:hover
{
    background: #507030;
    cursor: pointer;
}

input[type="text"]
{
    width: 150px;
    color: #507030;
}

input[type="text"][readonly="readonly"]
{
    background-color: #BBDDBB;
}

legend
{
    margin-left: 10px;
    padding-left: 5px;
    padding-right: 5px;
}

tbody
{
    background-color: #FFFFFF;
}

#botones
{
    float: right;
    clear: both;
}

#footer
{
    padding-top: 20px;
}

#footer ul li
{
    display: inline-block;
    width: 390px;
    padding-top: 5px;
    padding-bottom: 4px;
    font-family: Calibri;
    font-size: 14px;
}

#footer ul li.footer-left
{
    padding-left: 10px;
}


#footer ul li.footer-right
{
    text-align: right;
    padding-right: 10px;
}

#Grid
{
    background-color: transparent;
}

#header
{
    width: 800px;
    height: 120px;
    background: #90C068;
}

#kendotable
{
    margin-top: 15px;
    max-width: 710px;
    overflow-x: auto;
}

#logo
{
    padding: 15px 0 0 15px;
}

