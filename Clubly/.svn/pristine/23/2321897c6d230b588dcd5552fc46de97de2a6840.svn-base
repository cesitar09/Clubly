#menubar
{
    width: 800px;
    height: 26px;
    background: transparent;
    margin-top: -26px;
}

#menubar a
{
    text-decoration: none;
    color: #FFFFFF;
    font-size: 16px;
    font-family: Calibri;
}

#menubar ul
{
    list-style: none;
}

#menubar ul li
{
    display: inline-block;
    background: #608840;
    width: 130px;
    text-align: center;
    padding-bottom: 7px;
}

#menubar ul li:hover
{
    background: #507030;
}

#menubar ul li.main
{
    border-top-right-radius: 10px;
}

#menubar ul li.logout
{
    border-top-left-radius: 10px;
    margin-left: 540px;
}

#page
{
    padding: 15px 45px 0 45px;
    min-height: 360px;
}

#page a:link
{
    font-weight: bold;
}

#page a:hover
{
    text-decoration: underline;
}

#Grid a:link, #Grid a:hover
{
    font-weight: normal;
    text-decoration: none;
}

#validacion
{
    color: #A02000;
    font-size: 10px;
    width: 339px;
    position: relative;
    left: 170px;
}

#validationSummary
{
    color: #FF2200;
    font-size: 12px;
}

#wrapper
{
    position: relative;
    width: 800px;
    background: #D0E094;
    margin: 0 auto 0 auto;
}

.editor-label > label:after
{
    content : " :";
}

.k-filter-menu li.k-item
{
    width: inherit;
}

.k-multiselect, .k-combobox
{
    width: auto;
    margin-left: 170px;
    margin-right: 20px;
}

.k-multiselect
{
    font-family: Times New Roman;
}

.k-multiselect li
{
    width:auto;
}